{
  "name": "@zelgadis87/ml.js",
  "version": "1.0.0-rc.0",
  "description": "A module loader with dependency injection",
  "main": "index.js",
  "scripts": {
    "lint": "npm run eslint",
    "eslint": "npx eslint -c .eslintrc ./src/**/*.js ./test/**/*.js",
    "test": "npm run eslint -- --rule=mocha/no-exclusive-tests:error --rule=mocha/no-pending-tests:error --rule=mocha/no-skipped-tests:error && npm run test-dev",
    "test-dev": "npx mocha ./test/**/*.spec.js",
    "test-dev-watch": "npm run test-dev -- --watch",
    "coverage": "npx nyc npm run test",
    "postcoverage": "npx nyc report --reporter=lcov",
    "versionate": "npx npm-versionator",
    "postversion": "npx codecov --file=coverage/lcov.info --token=aff1c748-1010-4aeb-a657-32ff67293d37"
  },
  "author": "Zelgadis87",
  "license": "ISC",
  "keywords": [
    "module",
    "module-loader",
    "dependency-injection",
    "ioc"
  ],
  "dependencies": {
    "bluebird": "^3.5.1",
    "lodash": "^4.17.5",
    "parse-function": "^5.0.23"
  },
  "devDependencies": {
    "@zelgadis87/npm-versionator": "^1.0.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.17.0",
    "eslint-plugin-mocha": "^4.11.0",
    "mocha": "^5.0.0",
    "nyc": "^11.4.1"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "test/files/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zelgadis87/ML.js.git"
  },
  "bugs": {
    "url": "https://github.com/Zelgadis87/ML.js/issues"
  },
  "homepage": "https://github.com/Zelgadis87/ML.js#readme"
}
